generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model JobPost {
  id          String   @id @default(uuid())
  title       String
  slug        String   @unique
  category    String
  description String
  content     String
  applyLink   String
  deadline    String
  createdAt   DateTime @default(now())
}
